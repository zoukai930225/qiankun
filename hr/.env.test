# 生产环境：只在打包时使用
NODE_ENV=production
ENV=test
# 打包环境后续都用VITE_ENV
VITE_ENV=test

VITE_DEV=false

# 请求路径
VITE_BASE_URL=''
VITE_BASE_URL_OTR=''
VITE_BASE_URL_AM=''
VITE_BASE_URL_DTS=''
VITE_BASE_URL_WORKFLOW_API=''

# 文件上传类型：server - 后端上传， client - 前端直连上传，仅支持S3服务
VITE_UPLOAD_TYPE=server



# 接口地址
VITE_API_URL=/admin-api

# 是否删除debugger
VITE_DROP_DEBUGGER=true

# 是否删除console.log
VITE_DROP_CONSOLE=true

# 是否sourcemap
VITE_SOURCEMAP=false

# 打包路径
VITE_BASE_PATH=/

# 输出路径
VITE_OUT_DIR=dist

# 自检
VITE_DETECTION=true